<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<PackageId>RandomCS</PackageId>
		<Version>1.0.0</Version>
		<TargetFramework>net9.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<Authors>Ben Schneider</Authors>
		<Company>Ben Schneider</Company>
		<Description>A library providing fast, secure, and flexible random number generation utilities</Description>
		
		<PackageProjectUrl>https://github.com/Ben-Scr/RandomCS</PackageProjectUrl>
		<RepositoryUrl>https://ko-fi.com/benscr</RepositoryUrl>
		<RepositoryType>git</RepositoryType>

		<PackageTags>
			Randomization;
			Cryptography;
			Performant;
		</PackageTags>
		
		<PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageIcon>icon.png</PackageIcon>
	</PropertyGroup>


	<ItemGroup>
		<None Include="../README.md" Pack="true" PackagePath="\" />
		<None Include="../LICENSE.txt" Pack="true" PackagePath="\" />
		<None Include="icon.png" Pack="true" PackagePath="\" />
	</ItemGroup>
</Project>
